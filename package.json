{
    "name": "clickup-dashboard-manager",
    "private": true,
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "dev": "npm run dev --workspaces",
        "build": "npm run build --workspaces",
        "start": "npm run start --workspaces",
        "migrate:dev": "npm run migrate:dev --workspace=api",
        "seed": "npm run seed --workspace=api",
        "docker:up": "docker-compose -f infra/docker-compose.yml up -d",
        "docker:down": "docker-compose -f infra/docker-compose.yml down"
    },
    "devDependencies": {
        "typescript": "^5.0.0"
    },
    "dependencies": {
        "@prisma/client": "^7.4.0",
        "prisma": "^7.4.0"
    }
}
